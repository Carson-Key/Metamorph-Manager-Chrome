{
    "name": "Metamorph Extension",
    "description": "A Chrome extension used to manage the M&M metamorph power in Roll20",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [
		"activeTab",
        "tabs"
	],
    "content_scripts": [
        {
          "matches": ["https://*.roll20.net/*"],
          "js": ["jquery.min.js", "index.js"]
        }
    ]
  }